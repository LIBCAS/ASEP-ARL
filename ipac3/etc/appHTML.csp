===============================================================================
../#(data("BATNAME"))#/build.bat
-------------------------------------------------------------------------------
@ECHO OFF
CD ..
CD ..
CALL grunt i3s --app=printer
CALL grunt i3s --app=lov
CALL grunt i3 --app=#(data("BATNAME"))#
PAUSE
===============================================================================
../../Gruntfile.js
-------------------------------------------------------------------------------
"#(data("BATNAME"))#": {
    "dir": "#(data("BATNAME"))#",
    "file": "#(data("MAIN_JS"))#",
    "packet": basicFiles.concat(#(data("DIR_EXTJS"))#Files, RTLFiles, ['#(data("APP_DIR"))#/main.csp', '#(data("APP_DIR"))#/index.csp', '#(data("APP_DIR"))#/main.js', '#(data("APP_DIR"))#/main.css', '#(data("APP_DIR"))#/s1/**', '#(data("APP_DIR"))#/images/**']),
    "jsfiles": [#(data("JSFILES"))#]
},
